{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
  "overrides": [
    {
      "files": "*.astro",
      "options": { "parser": "astro" }
    },
    {
      "files": "*.svelte",
      "options": { "parser": "svelte" }
    }
  ]
}
