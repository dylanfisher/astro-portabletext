---
export interface Props {
  onError: (error: string) => void;
}

const { onError } = Astro.props;

let html = "";

try {
  html = await Astro.slots.render("primary");
} catch (err) {
  onError(typeof err === "string" ? err : (err as Error).message);
  html = await Astro.slots.render("secondary");
}
---

<Fragment set:html={html} />
