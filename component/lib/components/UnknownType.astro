---
import { Props } from "../types";
import { unknownTypeWarning } from "../warnings";

const { node, isInline } = Astro.props as Props;
const { _type } = node;
const warning = unknownTypeWarning(_type);
---

{isInline ? (
  <span data-portabletext-unknown="type">{warning}</span>
) : (
  <div data-portabletext-unknown="type">{warning}</div>
)}
