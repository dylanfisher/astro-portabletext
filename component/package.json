{
  "name": "astro-portabletext",
  "version": "0.2.4",
  "type": "module",
  "description": "Render Portable Text with Astro",
  "keywords": [
    "astro",
    "astro-component",
    "portable-text",
    "typescript"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theisel/astro-portabletext.git",
    "directory": "component"
  },
  "bugs": {
    "url": "https://github.com/theisel/astro-portabletext/issues"
  },
  "homepage": "https://github.com/theisel/astro-portabletext#readme",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./utils": "./lib/utils.js",
    "./types": "./lib/types.d.ts"
  },
  "scripts": {
    "compile": "tsc",
    "build-fixture": "astro --root test/components/fixture/ build --silent",
    "prepublishOnly": "pnpm compile",
    "test-component": "pnpm build-fixture && npx uvu test/components .test.js",
    "test-lib": "npx uvu -r tsm test/lib ",
    "test-ts": "npx tsc --noEmit test/*.test-ts.ts",
    "test-all": "pnpm test-component && pnpm test-lib && pnpm test-ts",
    "test": "pnpm test-all"
  },
  "dependencies": {
    "@portabletext/toolkit": "^1.0.5",
    "@portabletext/types": "^1.0.3"
  },
  "devDependencies": {
    "cheerio": "1.0.0-rc.12",
    "ts-toolbelt": "^9.6.0",
    "tsm": "^2.2.2",
    "uvu": "^0.5.6"
  }
}
